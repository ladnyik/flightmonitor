#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/jsesc/0.5.0/node_modules/jsesc/bin/jsesc/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/jsesc/0.5.0/node_modules/jsesc/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/jsesc/0.5.0/node_modules/jsesc/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/jsesc/0.5.0/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/jsesc/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules:/home/ladnyik/vaadin/node_modules:/home/ladnyik/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../jsesc/0.5.0/node_modules/jsesc/bin/jsesc" "$@"
  ret=$?
else 
  NODE_PATH="/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/jsesc/0.5.0/node_modules/jsesc/bin/jsesc/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/jsesc/0.5.0/node_modules/jsesc/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/jsesc/0.5.0/node_modules/jsesc/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/jsesc/0.5.0/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/jsesc/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules:/home/ladnyik/vaadin/node_modules:/home/ladnyik/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../jsesc/0.5.0/node_modules/jsesc/bin/jsesc" "$@"
  ret=$?
fi
exit $ret
