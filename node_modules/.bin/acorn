#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/bin/acorn/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/acorn/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules:/home/ladnyik/vaadin/node_modules:/home/ladnyik/node_modules:/home/node_modules:/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/acorn/bin/acorn/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/acorn/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/acorn/node_modules" "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
  ret=$?
else 
  NODE_PATH="/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/bin/acorn/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/acorn/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules:/home/ladnyik/vaadin/node_modules:/home/ladnyik/node_modules:/home/node_modules:/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/acorn/bin/acorn/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/acorn/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/acorn/node_modules" node  "$basedir/../acorn/bin/acorn" "$@"
  ret=$?
fi
exit $ret
