#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/he/1.2.0/node_modules/he/bin/he/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/he/1.2.0/node_modules/he/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/he/1.2.0/node_modules/he/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/he/1.2.0/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/he/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules:/home/ladnyik/vaadin/node_modules:/home/ladnyik/node_modules:/home/node_modules:/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/he/bin/he/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/he/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/he/node_modules" "$basedir/node"  "$basedir/../he/bin/he" "$@"
  ret=$?
else 
  NODE_PATH="/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/he/1.2.0/node_modules/he/bin/he/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/he/1.2.0/node_modules/he/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/he/1.2.0/node_modules/he/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/he/1.2.0/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/he/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules:/home/ladnyik/vaadin/node_modules:/home/ladnyik/node_modules:/home/node_modules:/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/he/bin/he/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/he/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/he/node_modules" node  "$basedir/../he/bin/he" "$@"
  ret=$?
fi
exit $ret
