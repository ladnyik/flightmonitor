#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules/btoa/bin/btoa.js/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules/btoa/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules/btoa/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/btoa/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules:/home/ladnyik/vaadin/node_modules:/home/ladnyik/node_modules:/home/node_modules:/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/btoa/bin/btoa.js/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/btoa/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/btoa/node_modules" "$basedir/node"  "$basedir/../btoa/bin/btoa.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules/btoa/bin/btoa.js/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules/btoa/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules/btoa/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/btoa/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/.pnpm/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules:/home/ladnyik/vaadin/node_modules:/home/ladnyik/node_modules:/home/node_modules:/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/btoa/bin/btoa.js/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/btoa/bin/node_modules:/home/ladnyik/vaadin/flight-monitor/node_modules/btoa/node_modules" node  "$basedir/../btoa/bin/btoa.js" "$@"
  ret=$?
fi
exit $ret
